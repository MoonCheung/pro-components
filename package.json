{
  "name": "vue-antd-pro-components-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "engines": {
    "node": ">=12.2.0"
  },
  "homepage": "https://sunshineLixun.github.io/pro-components",
  "scripts": {
    "build": "pnpm run build --filter ./packages",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "TIMING=1 eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "preinstall": "npx only-allow pnpm",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "docs:deploy": "pnpm run docs:build && gh-pages -d docs/.vitepress/dist -b dev -r https://github.com/sunshineLixun/pro-components"
  },
  "keywords": [
    "antd",
    "vue",
    "ant-design-vue",
    "pro-components",
    "pro-layout",
    "pro-form"
  ],
  "author": "Sendya",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vueComponent/pro-components"
  },
  "contributors": [
    "Sendya <18x@loacg.com>",
    "tangjinzhou <415800467@qq.com>"
  ],
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.3",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.27",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.20",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.13.0",
    "eslint-plugin-vue": "^8.6.0",
    "gh-pages": "^4.0.0",
    "jsdom": "^19.0.0",
    "prettier": "^2.6.2",
    "typescript": "~4.5.5",
    "vite": "^2.9.5",
    "vitepress": "1.0.0-alpha.13",
    "vitest": "^0.5.9",
    "vue": "^3.2.38",
    "vue-tsc": "^0.34.7"
  },
  "lint-staged": {
    "*.{js,ts,vue,json}": [
      "pnpm lint"
    ]
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  }
}